    "autoload": {
        "files": [
            "src/Boot/src/helpers.php",
            "src/Framework/helpers.php",
            "src/Scaffolder/src/helpers.php",
            "src/Stempler/src/helpers.php",
            "src/Translator/src/helpers.php"
