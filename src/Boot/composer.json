{
    "name": "spiral/boot",
    "type": "library",
    "description": "Environment Bootloader and Dispatch Manager",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/boot"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "spiral/core": "^2.13",
        "spiral/files": "^2.13",
        "spiral/config": "^2.13",
        "spiral/debug": "^2.13",
        "spiral/exceptions": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.5",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Spiral\\Boot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Boot\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.13.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
