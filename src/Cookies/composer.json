{
    "name": "spiral/cookies",
    "type": "library",
    "description": "Spiral Framework: Secure Cookie middleware",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/cookies"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "spiral/core": "^2.14",
        "spiral/encrypter": "^2.14",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.5",
        "mockery/mockery": "^1.5",
        "spiral/http": "^2.14",
        "laminas/laminas-diactoros": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Cookies\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Cookies\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.14.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
