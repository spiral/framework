{
    "name": "spiral/scaffolder",
    "type": "library",
    "license": "MIT",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spiral"
        }
    ],
    "description": "Spiral, Code Scaffolding module",
    "keywords": ["scaffolding"],
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/scaffolder"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        },
        {
            "name": "Pavel Butchnev (butschster)",
            "email": "pavel.buchnev@spiralscout.com"
        },
        {
            "name": "Aleksei Gagarin (roxblnfk)",
            "email": "alexey.gagarin@spiralscout.com"
        },
        {
            "name": "Maksim Smakouz (msmakouz)",
            "email": "maksim.smakouz@spiralscout.com"
        },
        {
            "name": "Valentin Vintsukevich (vvval)",
            "email": "valentin@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "spiral/reactor": "^3.14.6",
        "cocur/slugify": "^3.2 || ^4.0",
        "doctrine/inflector": "^1.4|^2.0",
        "nette/php-generator": "^4.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "spiral/framework": "^3.1",
        "vimeo/psalm": "^5.9"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Spiral\\Scaffolder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Scaffolder\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.15.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
