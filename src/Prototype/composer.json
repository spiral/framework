{
    "name": "spiral/prototype",
    "type": "library",
    "description": "Automatic dependency injection",
    "license": "MIT",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spiral"
        }
    ],
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/prototype"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        },
        {
            "name": "Pavel Butchnev (butschster)",
            "email": "pavel.buchnev@spiralscout.com"
        },
        {
            "name": "Aleksei Gagarin (roxblnfk)",
            "email": "alexey.gagarin@spiralscout.com"
        },
        {
            "name": "Maksim Smakouz (msmakouz)",
            "email": "maksim.smakouz@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "nikic/php-parser": "^5.4",
        "doctrine/inflector": "^1.4|^2.0",
        "spiral/attributes": "^2.8|^3.0",
        "spiral/config": "^3.15.7",
        "spiral/reactor": "^3.15.7"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Prototype\\": "src"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.6.12",
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^10.5.41",
        "spiral/dumper": "^3.3",
        "vimeo/psalm": "^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Prototype\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.15.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
