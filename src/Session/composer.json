{
    "name": "spiral/session",
    "type": "library",
    "description": "Secure sessions over native PHP handlers",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/session"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "spiral/core": "^2.14",
        "spiral/files": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Session\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Session\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.14.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
